{
  "name": "@incutonez/zelda-ts",
  "description": "Legend of Zelda in TypeScript using Phaser 3",
  "version": "1.2.1",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/incutonez/ZeldaTS.git"
  },
  "author": "incutonez@hotmail.com",
  "license": "MIT",
  "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
  "keywords": [
    "phaser",
    "phaser3",
    "typescript",
    "vue",
    "vite"
  ],
  "scripts": {
    "dev": "vite --config vite/config.dev.js",
    "build": "vite build --config vite/config.prod.js",
    "dev-nolog": "vite --config vite/config.dev.js",
    "build-nolog": "vite build --config vite/config.prod.js",
    "prepare": "husky"
  },
  "dependencies": {
    "@volar/language-server": "2.4.0-alpha.18",
    "@vue/language-server": "2.0.28",
    "phaser": "^3.85.1",
    "vue": "^3.4.27"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@incutonez/eslint-plugin": "^1.1.4",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@stylistic/eslint-plugin-ts": "^2.7.2",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.11.20",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.10.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "eslint-plugin-vue": "^9.28.0",
    "globals": "^15.9.0",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.45",
    "semantic-release": "^24.1.0",
    "tailwindcss": "^3.4.10",
    "terser": "^5.31.0",
    "typescript": "~5.3.3",
    "typescript-eslint": "^8.4.0",
    "vite": "^5.2.11",
    "vue-tsc": "^2.0.6"
  },
  "lint-staged": {
    "*.{js,mjs,cjs,jsx,ts,tsx,vue}": [
      "npx eslint --fix"
    ]
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  }
}
